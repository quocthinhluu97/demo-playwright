{
    "compilerOptions": {
        "target": "es6",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@pages/*": [
                "./pages/*"
            ],
            "@services/*": [
                "./services/*"
            ],
            "@helpers/*": [
                "./helpers/*"
            ],
            "@data/*": [
                "./data/*"
            ],
            "@utils/*": [
                "./utils/*"
            ],
            "@fixtures/*": [
                "./fixtures/*"
            ],
            "@constants/*": [
                "./constants/*"
            ],
            "@models/*": [
                "./models/*"
            ],
        }
    }
}